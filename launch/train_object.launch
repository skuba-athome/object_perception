<launch>
    <node pkg="object_perception" type="train_object.py" name="train_object" output="screen">
        <param name="object_directory" value="$(find object_perception)/data/"/>
        <!-- K-mean param -->
        <param name="kmean_max_iter" value="300"/>
        <param name="kmean_epsilon" value="0.000001"/>
        <param name="kmean_k_cluster" value="60"/>
        <param name="kmean_attempts" value="10"/>
        <!-- SVM param -->
        <param name="svm_c" value="2"/>
        <param name="svm_gamma" value="0.075"/>
    </node>
</launch>
